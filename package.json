{
  "name": "@konstant/stack",
  "version": "0.2.10",
  "type": "module",
  "files": ["dist"],
  "scripts": {
    "build": "pnpm ci:clean && pnpm tsup",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "ci:clean": "rimraf {dist,*.tsbuildinfo}",
    "ci:copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "ci:patch": "pnpm ci:version patch && pnpm ci:tag",
    "ci:tag": "node tools/scripts/tag.js",
    "ci:version": "node tools/scripts/ci-version.js",
    "ci:update-pkg": "node tools/scripts/update-publish-pkg.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "qs-esm": "^7.0.2",
    "slugify": "^1.6.6",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/eslintrc": "^3.3.1",
    "@payloadcms/eslint-config": "^3.28.0",
    "@payloadcms/next": "3.32.0",
    "@payloadcms/ui": "3.32.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4",
    "fs": "0.0.1-security",
    "next": "^15.2.4",
    "payload": "3.32.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rimraf": "^6.0.1",
    "semver": "^7.7.1",
    "sort-package-json": "^3.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
