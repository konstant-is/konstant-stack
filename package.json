{
  "name": "@konstant/stack",
  "version": "0.2.9",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm ci:clean && pnpm tsup",
    "build:types": "tsc --outDir dist --rootDir ./src",
    "ci:clean": "rimraf {dist,*.tsbuildinfo}",
    "ci:copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "ci:patch": "pnpm ci:version patch && pnpm ci:tag",
    "ci:tag": "node tools/scripts/tag.js",
    "ci:version": "node tools/scripts/ci-version.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "qs-esm": "^7.0.2",
    "slugify": "^1",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@payloadcms/eslint-config": "^3.9.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "fs": "0.0.1-security",
    "prettier": "^3",
    "rimraf": "^6.0.1",
    "semver": "^7.6.3",
    "sort-package-json": "^2.10.0",
    "tsup": "^8.3.5",
    "typescript": "latest"
  },
  "peerDependencies": {
    "@payloadcms/next": "3.22.0",
    "@payloadcms/ui": "3.22.0",
    "date-fns": "^4.1.0",
    "next": "^15.1.5",
    "payload": "3.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
