{
  "name": "@konstant/stack",
  "version": "0.0.27",
  "type": "module",
  "scripts": {
    "build": "pnpm clean && tsup && pnpm build:styles",
    "clean": "rm -rf dist",
    "build:styles": "sass theme.scss dist/theme.css --no-source-map",
    "ci:version-patch": "node scripts/ci-version.js patch",
    "prepare": "husky"
  },
  "files": [
    "dist",
    "src/styles",
    "theme.scss"
  ],
  "exports": {
    "./payload": "./dist/payload/index.js",
    "./payload/components": "./dist/payload/components.js",
    "./utils": "./dist/utils/index.js",
    "./utils/date": "./dist/utils/date.js",
    "./utils/string": "./dist/utils/string.js",
    "./utils/object": "./dist/utils/object.js",
    "./theme.scss": "./theme.scss"
  },
  "dependencies": {
    "@payloadcms/ui": "latest",
    "date-fns": "^3.6.0",
    "payload": "latest",
    "postcss": "^8.4.49",
    "qs": "^6.13.0",
    "react": "^19.0.0",
    "rimraf": "^6.0.1",
    "slugify": "^1.6.6",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^22.10.2",
    "@types/qs": "^6.9.15",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "esbuild-sass-plugin": "^3.3.1",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "sass": "^1.83.0",
    "semver": "^7.6.3",
    "tsup": "^8.3.5",
    "typescript": "latest"
  }
}
